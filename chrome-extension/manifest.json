{
    "manifest_version": 2,
  
    "name": "form-autofill",
    "description": "Tool for auto filling html forms based on the fake data generator",
    "version": "1.0",
    "icons":{
        "128":"icon.png"
    },  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "index.html"
    },
    "permissions": [
     "activeTab"
     ]
  }